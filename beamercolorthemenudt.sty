\definecolor{nudtblack}{RGB}{0,0,0}
\definecolor{nudtwhite}{RGB}{255,255,255}
\definecolor{nudtlightgrey}{RGB}{200,200,200}
\definecolor{nudtlightergrey}{RGB}{230,230,230}
\definecolor{nudtdarkgrey}{RGB}{72,72,72}
\definecolor{nudtblue}{RGB}{0,103,186}
\definecolor{nudtlyellow}{RGB}{202,108,24}
\definecolor{nudtgreen}{RGB}{0,127,127}
\definecolor{AMUDarkBlue}{RGB}{0,103,186}
\definecolor{AMULightBlue}{RGB}{110,188,253}
\definecolor{AMUYellow}{RGB}{0,127,127}

%\setbeamerfont{<bleh>}{family=, size=, shape=, series=}

\setbeamercolor{frametitle}{bg=nudtlightergrey,fg=nudtblue!70!black}

\setbeamercolor{titlepage}{bg=nudtlightgrey}
\setbeamercolor{titlepage main}{bg=nudtwhite,fg=nudtdarkgrey}

\setbeamercolor{headerline title}{fg=nudtwhite,bg=nudtblack}
\setbeamercolor{headerline border}{bg=nudtblue!70!black}

\setbeamercolor{footerline topborder}{bg=nudtblue}
\setbeamercolor{footerline}{bg=nudtdarkgrey,fg=nudtwhite}

\setbeamercolor{section in head/foot}{fg=nudtwhite}
\setbeamertemplate{section in head/foot shaded}[default][50]
\setbeamerfont{section in head/foot}{size=\tiny, shape=\bfseries}

\setbeamercolor{alerted text}{fg=nudtlyellow}
\setbeamercolor{background canvas}{bg=white}
\setbeamercolor{structure}{bg=nudtwhite, fg=nudtlyellow}

% blocks
\setbeamercolor{block title}          {bg=nudtlightergrey,fg=nudtdarkgrey}
\setbeamercolor{block body}           {fg=nudtblack}
\setbeamercolor{block title example}  {bg=nudtlightgrey,fg=nudtblue}
\setbeamercolor{block body example}   {bg=nudtlightergrey,fg=nudtdarkgrey}
\setbeamercolor{block title alerted}  {bg=nudtlyellow, fg=nudtblack}
\setbeamercolor{block body alerted}   {bg=nudtlightergrey,fg=nudtdarkgrey}
